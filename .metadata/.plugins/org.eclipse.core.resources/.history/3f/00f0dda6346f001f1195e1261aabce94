
public class Operator02 {
	public static void main(String[] args) {
		int a = 10;
		double b = 2.0;
		
		System.out.println("a + b = " + (a + b));
		System.out.println("a - b = " + (a - b));
		System.out.println("a * b = " + (a * b));
		System.out.println("a / b = " + (a / b));
		System.out.println("a % b = " + (a % b));
		
		int c = 10;
		double d = 3.0;
		System.out.println("***********************************************************");
		System.out.println("c + d = " + (c + d));
		System.out.println("c - d = " + (c - d));
		System.out.println("c * d = " + (c * d));
		System.out.println("c / d = " + (c / d));
		System.out.println("c % d = " + (c % d));
	}
}
