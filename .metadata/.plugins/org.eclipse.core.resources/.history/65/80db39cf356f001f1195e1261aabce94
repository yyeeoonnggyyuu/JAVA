

public class Operator03 {
	public static void main(String[] args) {
		
		int x =10;
		

		System.out.println((x <10 && x <20));
		System.out.println((x <10 || x <20));
		System.out.println((x <10 != x <20));
		System.out.println("***********************************************************");
		
		int c =5, d =10;

		System.out.println(c > d);
		System.out.println(!(c > d));
		System.out.println((c == d) && ( c == 5));
		System.out.println((c != d) && (c == 5));
		System.out.println(!((c!= d ) && (c == 5)));
	
	}
}
