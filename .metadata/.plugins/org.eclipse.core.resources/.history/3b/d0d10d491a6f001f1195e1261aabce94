
public class Welcome {

	public static void main(String[] args) {
		
		System.out.println("Welcome to Shooping Mall!");
	
		System.out.println("Welcome to Book Market!");
		
		int a = 10;
		int b = 20;
		int c = a+b;
	
		System.out.println(a+b);
		System.out.println(c);
		int d = ' ';
		
		for(int i = 0; i<10 ; i++) {
			System.out.print("*");
		}
		
	}

}
