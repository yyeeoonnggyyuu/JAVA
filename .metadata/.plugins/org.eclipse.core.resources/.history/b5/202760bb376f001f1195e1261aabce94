
public class TypeConversion01 {

	public static void main(String[] args) {
//		int a = 10;
//		double b =3.0;
//		
//		System.out.println("a + b = " + (a + b));
//		System.out.println("a - b = " + (a - b));
//		System.out.println("a * b = " + (a * b));
//		System.out.println("a / b = " + (a / b));
//	
//	
		int a = 10;
		int b =3;
		
		System.out.println("a + b = " + (a + b));
		System.out.println("a - b = " + (a - b));
		System.out.println("a * b = " + (a * b));
		System.out.println("a / b = " + (a / b));
	}
	

}

